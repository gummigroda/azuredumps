          "name": "2c79b4af-f830-b61e-92b9-63dfa30f16e4",
          "existenceCondition": {
            "field": "Microsoft.Security/assessments/status.code",
            "in": [
              "NotApplicable",
              "Healthy"
            ]
          }
        }
      }
    },
    "PolicyType": 2
  },
